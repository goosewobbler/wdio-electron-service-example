{
  "name": "@wdio-electron-service-example/cjs",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "build": "webpack && cp ./src/index.html ./dist && electron-builder -p never",
    "ci": "pnpm build && pnpm test && pnpm lint && pnpm check",
    "ci:logs": "cat ./wdio-logs/wdio-0-0.log && cat ./wdio-logs/wdio-0-1.log && cat ./wdio-logs/wdio-0-2.log && cat ./wdio-logs/wdio-0-3.log",
    "clean": "rm -rf ./dist ./wdio-logs ./node_modules pnpm-lock.yaml",
    "format": "biome format --write .",
    "check": "biome check .",
    "lint": "eslint \"test/**/*.{j,t}s\"",
    "test": "cross-env DEBUG=wdio-electron-service:* wdio run ./wdio.conf.js"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/spy": "^3.2.4",
    "@wdio/cli": "^9.19.1",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.19.1",
    "@wdio/mocha-framework": "^9.19.1",
    "cross-env": "^10.0.0",
    "debug": "^4.4.1",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-wdio": "^9.16.2",
    "expect-webdriverio": "5.4.2",
    "global-jsdom": "^26.0.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "wdio-electron-service": "9.0.0-next.5",
    "webdriverio": "^9.19.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "build": {
    "asar": true,
    "appId": "com.wdio-electron-service-example-cjs.demo",
    "copyright": "goosewobbler",
    "productName": "wdio-electron-service-example-cjs",
    "files": ["./dist/*"],
    "linux": {
      "executableName": "wdio-electron-service-example-cjs",
      "category": "Utility",
      "target": ["AppImage"]
    }
  }
}
