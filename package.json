{
  "name": "wdio-electron-service-example",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "build": "webpack && cp ./src/index.html ./dist && electron-builder -p never",
    "ci": "pnpm build && pnpm test && pnpm lint",
    "ci:logs": "cat ./wdio-logs/wdio-0-0.log && cat ./wdio-logs/wdio-0-1.log && cat ./wdio-logs/wdio-0-2.log && cat ./wdio-logs/wdio-0-3.log",
    "clean": "pnpm clean:dist && rm -rf ./node_modules ./all-logs pnpm-lock.yaml",
    "clean:dist": "rm -rf ./dist && mkdir -p ./dist",
    "format": "prettier --write \"**/*.{j,t}s\"",
    "lint": "eslint \"**/*.{j,t}s\"",
    "prepare": "husky",
    "test": "xvfb-maybe wdio run ./wdio.conf.js"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitest/spy": "^3.1.2",
    "@wdio/cli": "^9.15.0",
    "@wdio/globals": "^9.12.7",
    "@wdio/local-runner": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "debug": "^4.4.0",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-wdio": "^9.9.1",
    "expect-webdriverio": "5.1.0",
    "global-jsdom": "^26.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "wdio-electron-service": "8.1.0",
    "webdriverio": "^9.12.7",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "xvfb-maybe": "^0.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix"
    ],
    "**/*.{yml,json,md,ts,js}": [
      "prettier --write"
    ]
  },
  "build": {
    "asar": true,
    "appId": "com.wdio-electron-service-example.demo",
    "copyright": "goosewobbler",
    "productName": "wdio-electron-service-example",
    "files": [
      "./dist/*"
    ],
    "linux": {
      "executableName": "wdio-electron-service-example",
      "category": "Utility",
      "target": [
        "AppImage"
      ]
    }
  },
  "packageManager": "pnpm@10.3.0+sha512.ee592eda8815a8a293c206bb0917c4bb0ff274c50def7cbc17be05ec641fc2d1b02490ce660061356bd0d126a4d7eb2ec8830e6959fb8a447571c631d5a2442d",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "edgedriver",
      "esbuild",
      "geckodriver"
    ],
    "onlyBuiltDependencies": [
      "electron"
    ]
  }
}
